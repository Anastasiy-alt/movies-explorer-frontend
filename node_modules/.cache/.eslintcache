   });\n    // }\n\n    return (\n        <div className=\"profile\">\n            <h1 className=\"profile__title\">Привет, {name}!</h1>\n            <form className=\"profile__form\">\n                <label className='profile__label'>Имя\n                    <input\n                        onChange={handleNameChange}\n                        value={name || ''}\n                        type=\"text\"\n                        className=\"profile__item profile__item_name\"\n                        name=\"name\"\n                        required />\n                </label>\n                <label className='profile__label'>E-mail\n                    <input\n                        onChange={handleEmailChange}\n                        value={email || ''}\n                        type=\"url\"\n                        className=\"profile__item profile__item_email\"\n                        name=\"email\"\n                        required />\n                </label>\n            </form>\n            <div className=\"profile__links\">\n                <button className='profile__edit button' type='submit'>Редактировать</button>\n                <Link className='profile__logout link' onClick={onSignOut} to=\"/signup\">Выйти из аккаунта</Link>\n            </div>\n        </div>\n    )\n}\n\nexport default Profile;",{"ruleId":"135","severity":1,"message":"136","line":27,"column":10,"nodeType":"137","messageId":"138","endLine":27,"endColumn":19},{"ruleId":"139","severity":1,"message":"140","line":48,"column":6,"nodeType":"141","endLine":48,"endColumn":8,"suggestions":"142"},{"ruleId":"135","severity":1,"message":"143","line":1,"column":10,"nodeType":"137","messageId":"138","endLine":1,"endColumn":14},{"ruleId":"135","severity":1,"message":"144","line":18,"column":17,"nodeType":"137","messageId":"138","endLine":18,"endColumn":21},{"ruleId":"135","severity":1,"message":"145","line":4,"column":12,"nodeType":"137","messageId":"138","endLine":4,"endColumn":18},{"ruleId":"135","s